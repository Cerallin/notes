{
  "name": "hexo-site",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "plugin": "for dir in $(pwd)/plugins/*; do plugin=${dir##*plugins/}; dest=\"$(pwd)/node_modules/$plugin\"; ln -s $dir $dest; done ",
    "new": "hexo new",
    "build": "hexo generate",
    "clean": "hexo clean",
    "deploy": "hexo clean && hexo deploy",
    "server": "while :; do hexo server; done"
  },
  "hexo": {
    "version": "8.1.1"
  },
  "dependencies": {
    "@cerallin/hexo-generator-word-cloud": "^1.0.5",
    "canvas": "^3.2.1",
    "ejs-lint": "^2.0.1",
    "hexo": "^8.1.1",
    "hexo-ai-description": "^1.0.2",
    "hexo-cdnify": "^1.1.0",
    "hexo-clean-css": "^2.0.0",
    "hexo-deployer-git": "^4.0.0",
    "hexo-filter-fix-cjk-spacing": "0.0.5",
    "hexo-filter-mathjax": "^0.9.1",
    "hexo-filter-text-autospace": "^1.2.5",
    "hexo-generator-archive": "^2.0.0",
    "hexo-generator-baidu-sitemap": "^0.1.9",
    "hexo-generator-category": "^2.0.0",
    "hexo-generator-feed": "^3.0.0",
    "hexo-generator-index": "^4.0.0",
    "hexo-generator-search": "^2.4.3",
    "hexo-generator-sitemap": "^3.0.1",
    "hexo-generator-tag": "^2.0.0",
    "hexo-html-minifier": "^1.0.0",
    "hexo-pagination": "^4.0.0",
    "hexo-renderer-pandoc": "^0.5.0",
    "hexo-renderer-pug": "^3.0.0",
    "hexo-renderer-stylus": "^3.0.1",
    "hexo-renderer-viz": "^1.0.1",
    "hexo-server": "^3.0.0"
  }
}
