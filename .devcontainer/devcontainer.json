{
    "name": "note-site",
    "dockerFile": "./Dockerfile",
    "remoteUser": "texlive",
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-vscode.cpptools",
                "streetsidesoftware.code-spell-checker",
                "yzhang.markdown-all-in-one",
